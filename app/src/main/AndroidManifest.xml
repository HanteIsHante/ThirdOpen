<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.hante.thirdopen">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".ThirdOpenApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".useractivity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ThirdOpenHomeActivity">
        </activity>
        <activity android:name=".useractivity.RegisterActivity">
        </activity>
        <activity android:name=".flashpage.FlashActivity">
        </activity>
    </application>

</manifest>